{
  "datasets": [
    {
      "name": "daily_efficiency_trend",
      "displayName": "Daily Operational Efficiency Trend",
      "queryLines": [
        "SELECT\n",
        "  operation_date,\n",
        "  active_trucks,\n",
        "  total_events,\n",
        "  total_operational_minutes,\n",
        "  productive_time_percentage,\n",
        "  breakdown_time_percentage,\n",
        "  delay_time_percentage\n",
        "FROM mining_operations.production.gold_daily_operational_efficiency\n",
        "WHERE operation_date >= CURRENT_DATE - INTERVAL 30 DAYS\n",
        "ORDER BY operation_date DESC"
      ]
    },
    {
      "name": "truck_performance_ranking",
      "displayName": "Truck Performance Ranking",
      "queryLines": [
        "SELECT\n",
        "  truck_id,\n",
        "  truck_type,\n",
        "  total_events,\n",
        "  total_operational_minutes,\n",
        "  productive_time_percentage,\n",
        "  breakdown_time_percentage,\n",
        "  delay_time_percentage,\n",
        "  RANK() OVER (ORDER BY productive_time_percentage DESC) as efficiency_rank\n",
        "FROM mining_operations.production.gold_truck_performance\n",
        "ORDER BY productive_time_percentage DESC"
      ]
    },
    {
      "name": "efficiency_rating_distribution",
      "displayName": "Efficiency Rating Distribution",
      "queryLines": [
        "SELECT\n",
        "  efficiency_rating,\n",
        "  COUNT(*) as event_count,\n",
        "  SUM(duration_minutes) as total_duration_minutes,\n",
        "  AVG(duration_minutes) as avg_duration_minutes,\n",
        "  SUM(lost_productivity_minutes) as total_lost_minutes\n",
        "FROM mining_operations.production.gold_events_with_time_usage\n",
        "WHERE event_timestamp_ts >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "GROUP BY efficiency_rating\n",
        "ORDER BY \n",
        "  CASE efficiency_rating\n",
        "    WHEN 'Excellent' THEN 1\n",
        "    WHEN 'Good' THEN 2\n",
        "    WHEN 'Fair' THEN 3\n",
        "    WHEN 'Poor' THEN 4\n",
        "    WHEN 'Critical' THEN 5\n",
        "  END"
      ]
    },
    {
      "name": "time_of_day_analysis",
      "displayName": "Time of Day Efficiency Analysis",
      "queryLines": [
        "SELECT\n",
        "  time_of_day,\n",
        "  COUNT(*) as total_events,\n",
        "  AVG(overall_efficiency_percentage) as avg_efficiency,\n",
        "  SUM(total_duration_minutes) as total_minutes,\n",
        "  SUM(total_effective_productive_minutes) as productive_minutes,\n",
        "  SUM(total_lost_productivity_minutes) as lost_minutes\n",
        "FROM mining_operations.production.gold_time_usage_efficiency_summary\n",
        "WHERE event_date >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "GROUP BY time_of_day\n",
        "ORDER BY \n",
        "  CASE time_of_day\n",
        "    WHEN 'Morning' THEN 1\n",
        "    WHEN 'Afternoon' THEN 2\n",
        "    WHEN 'Evening' THEN 3\n",
        "    WHEN 'Night' THEN 4\n",
        "  END"
      ]
    },
    {
      "name": "productivity_loss_analysis",
      "displayName": "Productivity Loss Analysis",
      "queryLines": [
        "SELECT\n",
        "  event_type,\n",
        "  event_category,\n",
        "  COUNT(*) as event_count,\n",
        "  SUM(duration_minutes) as total_duration,\n",
        "  SUM(lost_productivity_minutes) as total_lost_minutes,\n",
        "  AVG(target_efficiency_percentage) as avg_target_efficiency,\n",
        "  ROUND((SUM(lost_productivity_minutes) / SUM(duration_minutes)) * 100, 2) as loss_percentage\n",
        "FROM mining_operations.production.gold_events_with_time_usage\n",
        "WHERE event_timestamp_ts >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "  AND lost_productivity_minutes > 0\n",
        "GROUP BY event_type, event_category\n",
        "ORDER BY total_lost_minutes DESC\n",
        "LIMIT 15"
      ]
    },
    {
      "name": "truck_efficiency_comparison",
      "displayName": "Truck Efficiency Comparison",
      "queryLines": [
        "SELECT\n",
        "  truck_id,\n",
        "  truck_type,\n",
        "  productive_time_percentage,\n",
        "  breakdown_time_percentage,\n",
        "  delay_time_percentage,\n",
        "  CASE \n",
        "    WHEN productive_time_percentage >= 90 THEN 'Top Performer'\n",
        "    WHEN productive_time_percentage >= 80 THEN 'Good Performer'\n",
        "    WHEN productive_time_percentage >= 70 THEN 'Average Performer'\n",
        "    ELSE 'Needs Improvement'\n",
        "  END as performance_category\n",
        "FROM mining_operations.production.gold_truck_performance\n",
        "ORDER BY productive_time_percentage DESC"
      ]
    },
    {
      "name": "operational_insights",
      "displayName": "Operational Insights Summary",
      "queryLines": [
        "SELECT\n",
        "  'Total Events (7 days)' as metric_name,\n",
        "  COUNT(*) as metric_value,\n",
        "  'events' as unit\n",
        "FROM mining_operations.production.gold_events_with_time_usage\n",
        "WHERE event_timestamp_ts >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "UNION ALL\n",
        "SELECT\n",
        "  'Average Efficiency' as metric_name,\n",
        "  ROUND(AVG(target_efficiency_percentage), 1) as metric_value,\n",
        "  '%' as unit\n",
        "FROM mining_operations.production.gold_events_with_time_usage\n",
        "WHERE event_timestamp_ts >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "UNION ALL\n",
        "SELECT\n",
        "  'Total Lost Productivity' as metric_name,\n",
        "  ROUND(SUM(lost_productivity_minutes) / 60, 1) as metric_value,\n",
        "  'hours' as unit\n",
        "FROM mining_operations.production.gold_events_with_time_usage\n",
        "WHERE event_timestamp_ts >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "UNION ALL\n",
        "SELECT\n",
        "  'Critical Events' as metric_name,\n",
        "  COUNT(*) as metric_value,\n",
        "  'events' as unit\n",
        "FROM mining_operations.production.gold_events_with_time_usage\n",
        "WHERE event_timestamp_ts >= CURRENT_DATE - INTERVAL 7 DAYS\n",
        "  AND efficiency_rating = 'Critical'"
      ]
    }
  ],
